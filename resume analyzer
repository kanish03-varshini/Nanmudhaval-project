        "\n",
        "prompt = PromptTemplate.from_template(template)\n",
        "\n",
        "# STEP 7: Use new LangChain syntax (prompt | llm | print)\n",
        "chain = prompt | llm\n",
        "response = chain.invoke({\"resume\": resume_text})\n",
        "\n",
        "# STEP 8: Show result\n",
        "print(\"Resume Analysis:\\n\")\n",
        "print(response.content)\n"
      ]
    }
  ]
}